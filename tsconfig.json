{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "rootDir": "./dist",
    "outDir": "./dist"
  },
  "exclude": [
    "dist/node_modules",
    "node_modules",
    "typings",
    "src"
  ]
}